{
  "name": "omelox-example-robot",
  "version": "2.1.0",
  "private": false,
  "description": "A distributed load test framework",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/gamingcity/omelox.git"
  },
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && cd dist &&  node app",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json **/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json **/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-loader lib/"
  },
  "dependencies": {
    "@types/node": "8.5.7",
    "@types/ws": "^3.2.0",
    "omelox-protobuf": "^2.1.0",
    "omelox-protocol": "^2.1.0",
    "omelox-robot": "^2.1.0",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "tslint": "^5.9.1",
    "typescript": "^2.9.2"
  }
}
