{
  "name": "omelox-example-robot",
  "version": "3.0.10",
  "private": false,
  "description": "A distributed load test framework",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/gamingcity/omelox"
  },
  "engines": {
    "node": ">=8.5"
  },
  "scripts": {
