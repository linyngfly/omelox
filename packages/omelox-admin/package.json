{
    "name": "omelox-admin",
    "version": "2.2.3",
    "main": "./dist/index",
    "types": "./lib/index",
    "files": [
        "dist",
        "lib",
        "sample",
        "test",
        "index.ts"
    ],
    "scripts": {
        "build": "tsc",
        "start": "yarn run build && node dist/index",
        "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
        "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
        "test": "yarn run build",
        "cov": "nyc mocha",
        "ci": "yarn run test",
        "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-admin lib/"
    },
    "engines": {
        "node": ">=8.5"
    },
    "dependencies": {
        "@types/mqtt-connection": "git+https://github.com/node-pinus/types-mqtt-connection.git",
        "@types/node": "12.13.0",
        "mqtt-connection": "^3.2.0",
        "omelox-logger": "^2.2.3",
        "omelox-monitor": "^2.2.3",
        "omelox-scheduler": "^2.2.3",
        "read-last-lines": "^1.3.0"
    },
    "devDependencies": {
        "mocha": ">=0.0.1",
        "should": ">=0.0.1",
        "tslint": "^5.9.1",
        "typescript": "^2.9.2"
    }
}
