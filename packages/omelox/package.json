{
  "name": "omelox",
  "version": "4.2.4",
  "author": "yng <linyngfly@163.com>",
  "private": false,
  "homepage": "https://github.com/linyngfly/omelox",
  "repository": {
    "type": "git",
    "url": "https://github.com/linyngfly/omelox.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "mochatest": "cd dist && mocha --require source-map-support/register --recursive",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox lib/"
  },
  "engines": {
    "node": ">=8.5"
  },
  "bugs": {
    "url": "https://github.com/linyngfly/omelox/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/linyngfly/omelox#license"
    }
  ],
  "keywords": [
    "omelox",
    "framework",
    "game",
    "web",
    "realtime",
    "server"
  ],
  "files": [
    "bin",
    "dist",
    "lib",
    "template",
    "test"
  ],
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "async": "^3.0.0",
    "cliff": "^0.1.10",
    "commander": "^8.0.0",
    "crc": "^3.8.0",
    "eyes": "^0.1.8",
    "mkdirp": "^1.0.4",
    "mqtt-connection": "^4.1.0",
    "node-bignumber": "^1.2.1",
    "omelox-admin": "^4.2.4",
    "omelox-loader": "^4.2.4",
    "omelox-logger": "^4.2.4",
    "omelox-protobuf": "^4.2.4",
    "omelox-protocol": "^4.2.4",
    "omelox-rpc": "^4.2.4",
    "omelox-scheduler": "^4.2.4",
    "seq-queue": "^0.0.5",
    "ws": "8.4.0"
  },
  "bin": {
    "omelox": "./dist/bin/omelox.js"
  },
  "devDependencies": {
    "@types/async": "3.2.8",
    "@types/cliff": "0.1.6",
    "@types/commander": "2.12.2",
    "@types/crc": "3.4.0",
    "@types/eyes": "0.1.32",
    "@types/mkdirp": "1.0.2",
    "@types/mocha": "^5.0.0",
    "@types/ws": "8.2.2",
    "mocha": "6.2.2",
    "muk": "0.5.3",
    "nyc": "15.1.0",
    "should": "13.2.3",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "gitHead": "6aaf063dd7e52db23772a5dd7e140bd436a46aac"
}
