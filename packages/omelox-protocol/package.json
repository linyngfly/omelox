{
 "name": "omelox-protocol",
 "version": "4.2.3",
 "author": "yng <linyngfly@163.com>",
 "private": false,
 "main": "./dist",
 "types": "./lib/index",
 "files": [
  "dist",
  "lib",
  "test",
  "index.ts"
 ],
 "scripts": {
  "build": "tsc",
  "start": "yarn run build && node dist/index",
  "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
  "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
  "test": "yarn run build",
  "cov": "nyc mocha",
  "ci": "yarn run test",
  "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-protocol lib/"
 },
 "homepage": "https://github.com/linyngfly/omelox",
 "repository": {
  "type": "git",
  "url": "https://github.com/linyngfly/omelox.git"
 },
 "engines": {
  "node": ">=8.5"
 },
 "devDependencies": {
  "@types/node": "16.11.17",
  "jscoverage": "0.6.0",
  "mocha": "6.2.2",
  "should": "13.2.3",
  "tslint": "^6.1.3",
  "typescript": "^4.3.5"
 },
 "gitHead": "6aaf063dd7e52db23772a5dd7e140bd436a46aac"
}
