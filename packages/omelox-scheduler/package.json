{
	"name": "omelox-scheduler",
	"version": "2.0.0",
	"author": "linyngfly <linyngfly@gmail.com>",
	"main": "./dist/schedule",
	"types": "./lib/schedule",
	"files": [
		"dist",
		"lib",
		"test"
	],
	"scripts": {
		"build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
		"fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-scheduler lib/"
	},
	"engines": {
		"node": ">=8.5"
	},
	"dependencies": {
		"@types/node": "8.5.7",
		"omelox-logger": "^2.0.0"
	},
	"devDependencies": {
		"tslint": "^5.9.1",
		"typescript": "^2.9.2"
	}
}
