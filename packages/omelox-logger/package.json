{
    "name": "omelox-logger",
    "version": "3.0.14",
    "author": "yng <linyngfly@163.com>",
    "private": false,
    "main": "./dist/index",
    "types": "./lib/index",
    "files": [
        "dist",
        "lib",
        "index.ts"
    ],
    "scripts": {
        "build": "tsc",
        "start": "yarn run build && node dist/index",
        "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
        "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
        "test": "yarn run build",
        "cov": "nyc mocha",
        "ci": "yarn run test",
        "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-logger lib/"
    },
    "homepage": "https://github.com/linyngfly/omelox",
    "repository": {
        "type": "git",
        "url": "https://github.com/linyngfly/omelox.git"
    },
    "engines": {
        "node": ">=8.5"
    },
    "dependencies": {
        "log4js": "^6.0.0"
    },
    "devDependencies": {
        "@types/node": "^12.12.6",
        "mocha": "6.2.2",
        "should": "13.2.3",
        "tslint": "5.20.1",
        "typescript": "^3.9.7"
    },
    "gitHead": "66bbfca6dc3e8deae2abb0fac2a93e44db3feaa7"
}
