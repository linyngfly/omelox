{
	"name": "omelox-loader",
	"version": "4.1.3",
	"author": "yng <linyngfly@163.com>",
	"private": false,
	"main": "./dist/index",
	"types": "./lib/index",
	"files": [
		"dist",
		"lib",
		"test",
		"index.ts"
	],
	"scripts": {
		"build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts",
		"fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-loader lib/"
	},
	"homepage": "https://github.com/linyngfly/omelox",
	"repository": {
		"type": "git",
		"url": "https://github.com/linyngfly/omelox.git"
	},
	"engines": {
		"node": ">=8.5"
	},
	"dependencies": {
		"reflect-metadata": "0.1.13"
	},
	"devDependencies": {
		"@types/node": "16.11.17",
		"mocha": "6.2.2",
		"should": "13.2.3",
		"tslint": "^6.1.3",
		"typescript": "^4.3.5"
	},
	"gitHead": "74380e10f86882a972286534fbfdaed864e54e7d"
}
