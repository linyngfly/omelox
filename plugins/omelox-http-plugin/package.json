{
  "name": "omelox-http-plugin",
  "version": "3.1.2",
  "description": "omelox http plugin",
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-monitor lib/"
  },
  "engines": {
    "node": ">=8.5"
  },
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "co-body": "^6.0.0",
    "debug": "^3.2.6",
    "ejs": "^2.7.4",
    "formidable": "^1.2.2",
    "koa": "^2.13.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.3",
    "koa-views": "^6.3.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/co-body": "^5.1.0",
    "@types/koa": "^2.11.4",
    "@types/koa-json": "^2.0.18",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.4",
    "@types/node": "^12.12.6",
    "@types/redis": "^2.8.27",
    "@types/yargs": "^13.0.0",
    "tslint": "5.20.1",
    "typescript": "^3.9.7"
  },
  "license": "MIT",
  "gitHead": "66bbfca6dc3e8deae2abb0fac2a93e44db3feaa7"
}
