{
  "name": "omelox-base-gate",
  "version": "4.2.2",
  "description": "omelox base gate plugin",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts --fix",
    "test": "npm run build",
    "cov": "nyc mocha",
    "ci": "npm run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/base-database src/"
  },
  "repository": "",
  "keywords": [
    "omelox"
  ],
  "main": "./dist/index",
  "types": "./src/index",
  "engines": {
    "node": ">=8.5"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "crc": "^3.8.0",
    "omelox": "^4.2.2",
    "omelox-robot-plugin": "^4.2.2",
    "reflect-metadata": "^0.1.10",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/crc": "^3.4.0",
    "@types/node": "16.11.17",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "license": "MIT",
  "publishConfig": {},
  "gitHead": "6d77f1a2b447b6e73f1540bb60504f3dc864b1fb"
}
