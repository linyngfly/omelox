{
  "name": "omelox-globalstatus-plugin",
  "version": "4.2.1",
  "description": "omelox globalstatus plugin",
  "files": [
    "dist",
    "src",
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json src/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-monitor lib/"
  },
  "engines": {
    "node": ">=8.5"
  },
  "main": "./dist/lib/index",
  "types": "./lib/index",
  "dependencies": {
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "16.11.17",
    "@types/redis": "^2.8.27",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "license": "MIT",
  "gitHead": "74380e10f86882a972286534fbfdaed864e54e7d"
}
