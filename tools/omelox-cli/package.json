{
  "name": "omelox-cli",
  "version": "4.2.1",
  "description": "omelox command line client for managing omelox",
  "scripts": {
    "build": "tsc",
    "start": "yarn run build && node dist/index",
    "fix-lint": "node_modules/.bin/tslint --format prose -c ../../tslint.json lib/**/*.ts test/**/*.ts --fix",
    "test": "yarn run build",
    "cov": "nyc mocha",
    "ci": "yarn run test",
    "gen-api-ref": "node ../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../run/site/public/api-reference/omelox-cli lib/"
  },
  "repository": "",
  "keywords": [
    "omelox",
    "command",
    "line",
    "maintening"
  ],
  "files": [
    "bin",
    "dist",
    "lib",
    "test",
    "index.ts"
  ],
  "main": "./dist/index",
  "bin": {
    "omelox-cli": "./dist/bin/omelox-cli.js"
  },
  "dependencies": {
    "async": "^3.0.0",
    "cliff": "^0.1.10",
    "omelox-admin": "^4.2.1",
    "omelox-logger": "^4.2.1",
    "optimist": "^0.6.1"
  },
  "devDependencies": {
    "@types/async": "3.2.8",
    "@types/cliff": "0.1.6",
    "@types/node": "16.11.17",
    "@types/optimist": "0.0.30",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "license": "MIT",
  "gitHead": "74380e10f86882a972286534fbfdaed864e54e7d"
}
